{
  "name": "TwineHacker",
  "version": "2.0.1.1",
  "description": "Allows you to change font/style; also adds cheats",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "file:///",
    "file://*/*",
    "file://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "devtools_page": "devtools.html",
  "options_page": "options.html"
}
